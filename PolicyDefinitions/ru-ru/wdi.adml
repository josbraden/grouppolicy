      <presentation id="WdiDpsScenarioExecutionPolicy">
        <dropdownList refId="WdiDpsScenarioExecutionPolicyLevel" noSort="true">Уровень выполнения сценария</dropdownList>
      </presentation>
    </presentationTable>
