      <presentation id="Pol_BackgroundSyncSettings">
        <text>Configurar sincronização em segundo plano:</text>
        <text> </text>
        <decimalTextBox refId="Lbl_BackgroundSyncDefaultSyncTime" defaultValue="360" spinStep="10">Intervalo de Sincronização (minutos)</decimalTextBox>
        <decimalTextBox refId="Lbl_BackgroundSyncVariance" defaultValue="60" spinStep="10">Variância de Sincronização (minutos)</decimalTextBox>
        <decimalTextBox refId="Lbl_BackgroundSyncIgnoreBlockOutTime" defaultValue="0" spinStep="15">Tempo Máximo Permitido Sem Sincronização (minutos)</decimalTextBox>
        <decimalTextBox refId="Lbl_BackgroundSyncBlockOutPeriodStartTime" defaultValue="0" spinStep="10">Hora de Início do Bloqueio (HHMM)</decimalTextBox>
