      <string id="IZ_ItemnameAnonymousLogon">Ouverture de session anonyme</string>
      <string id="IZ_ItemnameAutomaticLogon">Connexion automatique avec le nom d’utilisateur et le mot de passe actuels</string>
      <string id="IZ_ItemnameAutomaticLogonInIntranetZone">Connexion automatique uniquement dans la zone intranet</string>
