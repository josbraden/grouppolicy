      <!-- Overall category text -->
      <string id="PcaScenarioCategory">Anwendungskompatibilitätsdiagnose</string>
      

      <!-- Generic WDI text -->
      <string id="WdiScenarioExecutionPolicyLevelResolution">Erkennung, Problembehandlung und Konfliktlösung</string>
      <string id="WdiScenarioExecutionPolicyLevelTsOnly">Nur Erkennung und Problembehandlung</string>


      <!-- Individual scenario text -->
      <string id="DetectInstallFailuresText">Anwendungsinstallationsfehler erkennen</string>
      <string id="DetectUpdateFailuresText">Anwendungen erkennen, die unter der Benutzerkontensteuerung keine Installationsprogramme starten können</string>
      <string id="DetectDeprecatedComponentFailuresText">Durch veraltete Windows-DLLs verursachte Anwendungsfehler erkennen</string>
      <string id="DetectDeprecatedCOMComponentFailuresText">Durch veraltete COM-Objekte verursachte Anwendungsfehler erkennen</string>
      <string id="DetectUndetectedInstallersText">Anwendungsinstallationsprogramme erkennen, die als Administrator ausgeführt werden müssen</string>
      <string id="DetectBlockedDriversText">Über blockierte Treiber benachrichtigen</string>
