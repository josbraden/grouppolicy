        <textBox refId="ProxyAddress">
          <label>Proxyadresse</label>
        </textBox>
        <decimalTextBox refId="ProxyPort" defaultValue="80">Proxyport</decimalTextBox>
        <checkBox refId="BypassProxyLocal">Proxy für lokale Adressen umgehen</checkBox>
        <textBox refId="DoNotUseProxyLocal">
          <label>Proxyserver nicht verwenden für Adressen, die beginnen mit</label>
        </textBox>
      </presentation>
      <presentation id="ConfigureRTSPProxySettings">
