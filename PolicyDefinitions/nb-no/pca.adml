      <!-- Individual scenario text -->
      <string id="DetectInstallFailuresText">Oppdag programinstallasjonsfeil</string>
      <string id="DetectUpdateFailuresText">Oppdag programmer som ikke kan starte installasjonsprogram under brukertilgangskontroll</string>
      <string id="DetectDeprecatedComponentFailuresText">Oppdag programfeil forårsaket av avvergede Windows-DLLer</string>
      <string id="DetectDeprecatedCOMComponentFailuresText">Oppdag programfeil forårsaket av avvergede COM-objekter</string>
      <string id="DetectUndetectedInstallersText">Oppdag installasjonsprogrammer som krever kjøring som administrator</string>
