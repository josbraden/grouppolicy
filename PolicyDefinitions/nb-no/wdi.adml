      </presentation>
      <presentation id="WdiDpsScenarioExecutionPolicy">
        <dropdownList refId="WdiDpsScenarioExecutionPolicyLevel" noSort="true">Scenariokjøringsnivå</dropdownList>
      </presentation>
    </presentationTable>
