<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>Personvernpolicyer for operativsystemet</displayName>
  <description>Inneholder personvernpolicyer som gjelder for hele datamaskinen </description>
  <resources>
    <stringTable>
      <string id="PolicyPolicies">Policyer for operativsystem</string>
      <string id="EnableActivityFeed">Slår på aktivitetsfeeden</string>
      <string id="EnableActivityFeed_Help">
            Denne policyinnstillingen avgjør om ActivityFeed er aktivert.
            Hvis du aktiverer denne policyinnstillingen, kan alle (aktuelle) aktivitetstyper publiseres, og ActivityFeed distribuerer disse aktivitetene over hele brukerens enhetsgraf ved hjelp av roaming.
            Hvis du deaktiverer denne policyinnstillingen, kan ikke aktivitetene publiseres, og skysynkronisering av ActivityFeed deaktiveres også.
            Policyendringer trer i kraft umiddelbart.
      </string>
      <string id="PublishUserActivities">Tillat publisering av brukeraktiviteter</string>
      <string id="PublishUserActivities_Help">
            Denne policyinnstillingen avgjør om brukeraktiviteter kan publiseres.
            Hvis du aktiverer denne policyinnstillingen, kan aktiviteter av typen "Brukeraktivitet" publiseres.
            Hvis du deaktiverer denne policyinnstillingen, kan ikke aktiviteter av typen "Brukeraktivitet" publiseres.
            Policyendringer trer i kraft umiddelbart.
      </string>
      <string id="UploadUserActivities">Tillat opplasting av brukeraktiviteter</string>
      <string id="UploadUserActivities_Help">
            Denne policyinnstillingen avgjør om publiserte brukeraktiviteter kan lastes opp.
            Hvis du aktiverer denne policyinnstillingen, kan aktiviteter av typen Brukeraktivitet lastes opp.
            Hvis du deaktiverer denne policyinnstillingen, kan ikke aktiviteter av typen Brukeraktivitet lastes opp.
            Sletting av aktiviteter av typen Brukeraktivitet er uavhengige av denne innstillingen.
            Policyendringen trer i kraft umiddelbart.
      </string>
    </stringTable>
  </resources>
</policyDefinitionResources>
