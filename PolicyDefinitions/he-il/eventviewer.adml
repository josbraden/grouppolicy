<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>הזן שם תצוגה כאן</displayName>
  <description>הזן תיאור כאן</description>
  <resources>
    <stringTable>
      <string id="EventViewer">מציג האירועים</string>
      <string id="EventViewer_RedirectionProgram">התוכנית Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">זוהי התוכנית שתופעל כאשר המשתמש ילחץ על הקישור של Events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">פרמטרי שורת הפקודה של התוכנית Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">ציון פרמטרי שורת הפקודה שיועברו לתוכנית Events.asp</string>
      <string id="EventViewer_RedirectionURL">כתובת ה- URL של Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">זוהי כתובת ה- URL שתועבר לאזור התיאור בתיבת הדו-שיח 'מאפייני אירוע'.  שנה ערך זה אם ברצונך להשתמש בשרת אינטרנט שונה לשם טיפול בבקשות מידע של אירועים.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>התוכנית Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>פרמטרי שורת הפקודה של התוכנית Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>כתובת ה- URL של Events.asp</label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
