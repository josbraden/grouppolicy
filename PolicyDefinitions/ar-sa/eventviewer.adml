<policyDefinitionResources xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" revision="1.0" schemaVersion="1.0" xmlns="http://schemas.microsoft.com/GroupPolicy/2006/07/PolicyDefinitions">
  <displayName>أدخل اسم العرض هنا</displayName>
  <description>أدخل الوصف هنا</description>
  <resources>
    <stringTable>
      <string id="EventViewer">عارض الأحداث</string>
      <string id="EventViewer_RedirectionProgram">برنامج Events.asp</string>
      <string id="EventViewer_RedirectionProgram_Help">هذا هو البرنامج الذي سيتم استدعاؤه عند نقر المستخدم فوق ارتباط events.asp.</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters">معلمات سطر أوامر برنامج Events.asp</string>
      <string id="EventViewer_RedirectionProgramCommandLineParameters_Help">يحدد معلمات سطر الأوامر التي سيتم تمريرها إلى برنامج events.asp</string>
      <string id="EventViewer_RedirectionURL">محدد موقع معلومات Events.asp</string>
      <string id="EventViewer_RedirectionURL_Help">هذا هو محدد موقع المعلومات الذي سيتم تمريره إلى منطقة "الوصف" في مربع الحوار "خصائص الحدث"، وقم بتغيير هذه القيمة إذا أردت استخدام خادم "ويب" آخر لمعالجة طلبات معلومات الحدث.</string>
    </stringTable>
    <presentationTable>
      <presentation id="EventViewer_RedirectionProgram">
        <textBox refId="EventViewer_RedirectionProgram">
          <label>برنامج Events.asp</label>
          <defaultValue>%SystemRoot%\PCHealth\HelpCtr\Binaries\HelpCtr.exe</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionProgramCommandLineParameters">
        <textBox refId="EventViewer_RedirectionProgramCommandLineParameters">
          <label>معلمات سطر أوامر برنامج Events.asp</label>
          <defaultValue>-url hcp://services/centers/support?topic=%s</defaultValue>
        </textBox>
      </presentation>
      <presentation id="EventViewer_RedirectionURL">
        <textBox refId="EventViewer_RedirectionURL">
          <label>محدد موقع معلومات Events.asp </label>
          <defaultValue></defaultValue>
        </textBox>
      </presentation>
    </presentationTable>
  </resources>
</policyDefinitionResources>
