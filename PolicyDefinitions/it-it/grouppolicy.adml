      <presentation id="GPDCOptions">
        <dropdownList refId="GPDCOptionsDesc" noSort="true" defaultItem="0">Se Editor oggetti Criteri di gruppo sta selezionando un controller di dominio da utilizzare, è necessario che vengano eseguite le operazioni seguenti:</dropdownList>
      </presentation>
      <presentation id="GPTransferRate_1">
        <decimalTextBox refId="TransferRateOp1" defaultValue="500" spinStep="100">Velocità di connessione (Kbps):</decimalTextBox>
        <text> </text>
