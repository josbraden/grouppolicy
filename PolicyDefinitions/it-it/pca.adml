      <!-- Individual scenario text -->
      <string id="DetectInstallFailuresText">Rileva errori di installazione delle applicazioni</string>
      <string id="DetectUpdateFailuresText">Rileva le applicazioni che non sono in grado di avviare i programmi di installazione in controllo dell'account utente</string>
      <string id="DetectDeprecatedComponentFailuresText">Rileva errori delle applicazioni causati da DLL di Windows obsolete</string>
      <string id="DetectDeprecatedCOMComponentFailuresText">Rileva errori delle applicazioni causati da oggetti COM obsoleti</string>
      <string id="DetectUndetectedInstallersText">Rileva programmi di installazione delle applicazioni che richiedono l'esecuzione come amministratore</string>
