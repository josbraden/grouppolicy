    <stringTable>
    <string id="WindowsDefenderSecurityCenter">Windows Defender Security Center</string>    
    <string id="VirusThreatProtection">Ochrona przed wirusami i zagrożeniami</string>
    <string id="FirewallNetworkProtection">Zapora i ochrona sieci</string>
    <string id="AppBrowserProtection">Ochrona aplikacji i przeglądarki</string>
