      <string id="Help_NoNetscapeHelp">Menu Ajuda: Remover opção de menu 'Para Utilizadores do Netscape'</string>
      <string id="Help_NoTip">Menu Ajuda: Remover opção de menu 'Sugestão do Dia'</string>
      <string id="Help_NoTutorial">Menu Ajuda: Remover opção de menu 'Introdução'</string>
