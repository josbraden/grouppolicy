      <string id="IESF_CategoryProtectionFromZoneElevation">Protecção Contra Elevação de Zona</string>
      <string id="IESF_CategoryRestrictActiveXInstall">Restringir Instalação de ActiveX</string>
      <string id="IESF_CategoryRestrictFileDownload">Restringir Transferência de Ficheiros</string>
