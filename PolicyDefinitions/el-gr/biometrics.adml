      <presentation id="Biometrics_FUSTimeout">
          <decimalTextBox refId="Pol_Biometrics_FUSTimeout" defaultValue="10">Χρονικό όριο σε δευτερόλεπτα:</decimalTextBox>
      </presentation>
    </presentationTable>
