      <string id="Cat_OfflineFiles">Αρχεία χωρίς σύνδεση</string>
      <string id="Lbl_Fail">Ποτέ εργασία χωρίς σύνδεση</string>
      <string id="Lbl_FullSync">Πλήρης</string>
